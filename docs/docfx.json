{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/includes/**"
        ]
      }
    ],
    "markdownEngineName": "markdig",
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId":"MSDocsHeader-Xamarin",
      "breadcrumb_path": "/xamarin/breadcrumb/toc.json",
      "titleSuffix": "Xamarin",
      "extendBreadcrumb": true,
      "manager": "crdun",
      "audience": "developer",
      "ms.topic": "conceptual",
      "ms.technology": "xamarin-crossplatform",
      "ms.prod": "xamarin",
      "searchScope": ["Xamarin"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/xamarin-docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/"
    },
    "fileMetadata": {
      "ms.technology": {
        "essentials/**/**.md":"xamarin-essentials",
        "android/**/**.md":"xamarin-android",
        "android/**/**.yml":"xamarin-android",
        "ios/**/**.md":"xamarin-ios",
        "ios/**/**.yml":"xamarin-ios",
        "mac/**/**.md":"xamarin-mac",
        "mac/**/**.yml":"xamarin-mac",
        "xamarin-forms/**/**.md":"xamarin-forms",
        "xamarin-forms/**/**.yml":"xamarin-forms",
        "xamarin-essentials/**/**.md":"xamarin-forms",
        "xamarin-essentials/**/**.yml":"xamarin-forms",
        "get-started/quickstarts/**/**.md":"xamarin-forms",
        "get-started/tutorials/**/**.md":"xamarin-forms",
        "xamarin-forms/user-interface/graphics/skiasharp/**/**.md": "xamarin-skiasharp"
      },
      "feedback_product_url": {
        "essentials/**/**.md":"https://github.com/xamarin/Essentials/blob/master/PRODUCT-FEEDBACK.md"
      },
      "breadcrumb_path": {
        "get-started/tutorials/**/*.yml": "/xamarin/breadcrumb/tutorials/toc.json",
        "get-started/tutorials/index.yml": "/xamarin/breadcrumb/subtoc_breadcrumb/toc.json",
        "xamarin-forms/creating-mobile-apps-xamarin-forms/**/*.md": "/xamarin/breadcrumb/subtoc_breadcrumb/toc.json",
        "xamarin-forms/enterprise-application-patterns/**/*.md": "/xamarin/breadcrumb/subtoc_breadcrumb/toc.json",
        "xamarin-forms/user-interface/graphics/skiasharp/**/*.md": "/xamarin/breadcrumb/subtoc_breadcrumb/toc.json"
      },
      "searchScope": { 
        "android/**/**.md": ["Xamarin","Xamarin.Android"],
        "android/**/**.yml": ["Xamarin","Xamarin.Android"],
        "ios/**/**.md": ["Xamarin","Xamarin.iOS"],
        "ios/**/**.yml": ["Xamarin","Xamarin.iOS"],
        "mac/**/**.md": ["Xamarin","Xamarin.Mac"],
        "mac/**/**.yml": ["Xamarin","Xamarin.Mac"],
        "xamarin-forms/**/**.md": ["Xamarin","Xamarin.Forms"],
        "xamarin-forms/**/**.yml": ["Xamarin","Xamarin.Forms"],
        "xamarin-forms/user-interface/graphics/skiasharp/**/**.md": ["Xamarin","SkiaSharp"]
      }
    },
    "template": [],
    "dest": "xamarin-docs"
  }
}
